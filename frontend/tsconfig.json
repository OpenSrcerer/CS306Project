  "include": [
    "env.d.ts",
    "frontend/src/**/*",
    "frontend/src/**/*.vue"
  ],
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "..",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["node"]
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
